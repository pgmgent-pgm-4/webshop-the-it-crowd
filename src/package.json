    "express",
    "passport",
    "mvc",
    "middleware",
    "babel",
    "webpack",
    "eslint",
    "jest"
  ],
  "author": "Cagri Avci & Bram Criel",
  "license": "ISC",
  "homepage": "https://github.com/pgmgent-pgm-4/webshop-the-it-crowd.git#readme",
