{
  "name": "webshop-plant-it",
  "version": "1.0.0",
  "description": "plant webshop ecommerce",
  "main": "src/server/app.js",
  "scripts": {
    "dev": "cross-env NODE_ENV=development HOSTNAME=localhost PORT=6001 nodemon --exec babel-node -e js,html,nj server/app.js --watch server/*",
    "lint": "cross-env NODE_ENV=lint eslint . --ext .js --fix",
    "sass": "sass --style=compressed client/assets/sass/main.scss client/assets/css/main.css",
    "watch:sass": "npm run sass -- --watch",
    "watch:eleventy": "npx eleventy --config=.eleventy.cjs --serve",
    "start": "npm-run-all sass --parallel watch:*"
  },
